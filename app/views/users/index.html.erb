<table>
  <tr>
    <th>Имя</th>
    <th>Email</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%=h user.name %></td>
    <td><%= link_to 'Изменить', edit_user_path(user) %></td>
    <td><%= link_to 'Удалить', user, :confirm => 'Вы уверены?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Новый пользователь', new_user_path %>